pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                
                  sh 'echo "This is Build"'

            }
        }
        stage('Test') {
            steps {
                
                  sh 'echo "This id Test"'
                
            }
        }
        stage('Deploy') {
            steps {
                
                  sh 'echo "This is Deploy"'
                
            }
        }
    }
}

